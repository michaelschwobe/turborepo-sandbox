{
  "name": "vite-app",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf coverage && rm -rf dist && rm -rf dist-ssr && rm -rf node_modules",
    "lint": "yarn run lint:types && yarn run lint:scripts && yarn run lint:styles",
    "lint:types": "tsc -b",
    "lint:scripts": "TIMING=1 eslint src --ext=.js,.jsx,.ts,.tsx --fix",
    "lint:styles": "stylelint 'src/**/*.css' --allow-empty-input --fix --formatter=verbose",
    "format": "prettier '**/*' --write --ignore-unknown",
    "test": "jest --ci --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "dev": "vite --port=3400",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@acme/core": "*",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@acme/config": "*",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.5",
    "typescript": "^4.7.3",
    "vite": "^2.9.10"
  }
}
