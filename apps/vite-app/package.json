{
  "name": "vite-app",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf coverage && rm -rf dist && rm -rf dist-ssr && rm -rf node_modules",
    "lint": "yarn run lint:types && yarn run lint:scripts && yarn run lint:styles",
    "lint:types": "tsc --noEmit",
    "lint:scripts": "TIMING=1 eslint src --ext=.js,.jsx,.ts,.tsx --fix",
    "lint:styles": "stylelint 'src/**/*.css' --allow-empty-input --fix --formatter=verbose",
    "format": "prettier '**/*' --write --ignore-unknown",
    "dev": "vite --port=3400",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@acme/core": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@acme/config": "*",
    "@vitejs/plugin-react": "^1.2.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "eslint": "^8.10.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  }
}
