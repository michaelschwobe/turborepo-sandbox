{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",

  "extends": "./tsconfig.base.json",

  "compilerOptions": {
    "module": "ESNext",
    "resolveJsonModule": true,

    "noEmit": true,

    "allowJs": true,

    "incremental": true,

    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ESNext"]
  }
}
