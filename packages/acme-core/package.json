{
  "name": "@acme/core",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf coverage && rm -rf dist && rm -rf storybook-static && rm -rf node_modules",
    "lint": "yarn run lint:types && yarn run lint:scripts",
    "lint:types": "tsc -b",
    "lint:scripts": "TIMING=1 eslint src --fix",
    "test": "jest --ci --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "dev": "tsup src/index.ts --dts --format=esm,cjs --external=react --watch",
    "storybook": "start-storybook --ci --quiet -p 6006",
    "build": "tsup src/index.ts --dts --format=esm,cjs --external=react",
    "build-storybook": "build-storybook --quiet"
  },
  "dependencies": {
    "stop-runaway-react-effects": "^2.0.0"
  },
  "devDependencies": {
    "@acme/config": "*",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "stylelint": "^14.8.5",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3"
  }
}
