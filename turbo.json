{
  "baseBranch": "origin/main",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "postinstall": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "format": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "next-app#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "build/**", "out/**"]
    },
    "remix-app#build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/**", "build/**", "public/build/**"]
    },
    "vite-app#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "dist-ssr/**"]
    }
  }
}
